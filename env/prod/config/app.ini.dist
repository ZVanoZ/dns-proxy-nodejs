[app]
v4host=0.0.0.0
v6host=::
port=53

[dns-cache]
enabled=true
max-size=1000
max-ttl=86400
negative-ttl=60

[masked-dns]
"*.company.local"=company-dns
"*.local"=local-dns
"*"=inet-dns

[upstream-dns]
